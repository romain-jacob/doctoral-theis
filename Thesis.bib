  keywords = {Computer networks,Computerized monitoring,CPS,cyber-physical systems,Cyber-physical systems,Embedded computing,embedded systems,Feedback loop,feedback loops,Investments,object-oriented software,Physics computing,Process control,real time,Safety,Software quality,systems analysis,Yarn}
